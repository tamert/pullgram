{
   "background": {
      "persistent": true,
      "scripts": [ "js/jquery.js" ]
   },
   "content_scripts": [ {
      "js": [ "js/jquery.js", "js/content.js" ],
      "css": [ "css/pull-style.css" ],
      "matches": [ "https://www.instagram.com/*" ]
   } ],
   "description": "Multi follows, unfollows and unfollow users that does not following you",
   "icons": {
      "128": "img/128.png",
      "16": "img/16.png",
      "48": "img/48.png",
      "64": "img/64.png"
   },
   "manifest_version": 2,
   "homepage_url": "http://tinysay.com/",

   "browser_action": {
      "default_icon": "img/16.png",
      "default_title": "[INSERT WEB SITE NAME HERE] Fetcher",
      "default_popup": "popup.html"
   },
   "web_accessible_resources": [

      "img/*"
   ],
   "name": "Instagram Bulk Follow, UnFollow",
   "permissions": [ "storage", "tabs", "browsingData", "unlimitedStorage", "background", "http://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.0"
}

